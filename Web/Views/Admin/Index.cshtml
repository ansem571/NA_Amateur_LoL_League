@model string
@using Web.Views.Manage
@{
    ViewData["Title"] = "Admin page";
    ViewData.AddActivePage(ManageNavPages.Index);
}
<h4>@ViewData["Title"]</h4>
<div class="row">
    @Html.Partial("_StatusMessage", @Model)
</div>
<div class="row">
    <button onclick="@("window.location.href='" + Url.Action("CreateTeamAsync", "Admin") + "'");" class="btn">Create New Team</button>
    <button onclick="@("window.location.href='" + Url.Action("CreateCaptainAsync", "Admin") + "'");" class="btn">Assign Captain</button>
    <button onclick="@("window.location.href='" + Url.Action("UpdateTeamTierScoresAsync", "Admin") + "'");" class="btn">Update Roster scores</button>

</div>